<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.tnqls.spring.mapper.UserMapper">
 
 
<resultMap type="com.tnqls.spring.domain.User" id="userMap">
	<id property="userid" column="id"/>
	<result property="username" column="username"/>
	<result property="password" column="password"/>
	<result property="generated_date" column="generated_date"/>
</resultMap>

 <!-- 회원가입  -->
    <insert id="createUser">
        INSERT INTO user(userid, username, password) VALUES(#{userid}, #{username},#{password})
    </insert>
     <!-- 회원가입 
    <select id="getUserById" parameterType="string" resultType="com.tnqls.spring.domain.User">
    	SELECT * FROM user
    	WHERE userid = #{userid}
    </select>
      -->
</mapper>
